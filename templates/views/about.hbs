    {{>navbar}}
 <!-- Main Body Section -->
 <div class="container-fluid main">
            <div class="col-md-10 col-12 mx-auto">
                <div class="row d-flex justify-content-center align-items-center">
                        <div class="col-md-6 p-4">
                                    <figure>
                                                <img src="./Images/about.png" alt="about me" class="img-fluid">
                                    </figure>
                        </div>
                    <div class="col-md-6">
                        <p style="font-size: 1.5rem; margin-top: 5rem;">Welcome to Weather app</p>
                        <h1 class="weather-msg">I am a <span class="aboutme">Full Stack Developer</span> And a Freelancer</h1>
                        <button type="button" class="weather-btn mb-3">About Me</button>
                    </div>
                </div>
            </div>
        </div>

    <!-- Footer Section -->
    {{>footer}}
        
